Algoritmo "Exercicio 01"
Var
 nome, valor, s: real


Inicio
      escreva("Digite seu nome: ")
      leia(nome)
      escreva("Digite seu salario: ")
      leia(valor)
      
      s <- valor + (valor + (28.5/100))
      
      escreva("O meu nome é" ,nome, "E meu salário com aumento é",s)
      



Fimalgoritmo