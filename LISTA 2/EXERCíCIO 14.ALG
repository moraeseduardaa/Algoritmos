Algoritmo "Exercício 14"

Var
 n1,n2,diferenca,decimaParte:real


Inicio
    escreval("Digite a n1:")
    leia(n1)
    escreval("Digite a n2:")
    leia(n2)


diferenca <- n1 - n2

decimaParte <- diferenca / 10

    escreva("A décima parte dos valores dados é: ",decimaParte)

Fimalgoritmo