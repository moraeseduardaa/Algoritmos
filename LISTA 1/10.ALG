Algoritmo "Algoritmo"
//  
//  

Var
    a,c,l,z: real

Inicio

    escreval("Valor de a:")
    leia(a)
    escreval("Valor de c:")
    leia(c)
    escreval("Valor de l:")
    leia(l)



   z := ( l + a * 2) / c


    escreval("O resultado da fórmula de z = ( l + a * 2) / c é:",z)
    
  Fimalgoritmo