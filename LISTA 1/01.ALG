Algoritmo "01"

Var
  a,b,c,d: real

Inicio

    escreval("Primeiro valor:")
    leia(a)
    escreval("Segundo valor:")
    leia(b)
    escreval("Terceiro valor:")
    leia(c)



    d <- a + c - b


    escreval(" O resultado da formula d = a + c - b  é: ",d)
    
  Fimalgoritmo